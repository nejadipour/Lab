<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CalculatorController">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="New" />
                        <MenuItem mnemonicParsing="false" text="Open…" />
                        <Menu mnemonicParsing="false" text="Open Recent" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Close" />
                        <MenuItem mnemonicParsing="false" text="Save" />
                        <MenuItem mnemonicParsing="false" text="Save As…" />
                        <MenuItem mnemonicParsing="false" text="Revert" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Preferences…" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Quit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Undo" />
                        <MenuItem mnemonicParsing="false" text="Redo" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Cut" />
                        <MenuItem mnemonicParsing="false" text="Copy" />
                        <MenuItem mnemonicParsing="false" text="Paste" />
                        <MenuItem mnemonicParsing="false" text="Delete" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Select All" />
                        <MenuItem mnemonicParsing="false" text="Unselect All" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="sevenButton" layoutX="29.0" layoutY="66.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="7">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="cleanButton" layoutX="170.0" layoutY="21.0" mnemonicParsing="false" onAction="#cleanButtonController" text="AC" textFill="#ff6c00" />
                <ButtonBar layoutX="14.0" layoutY="14.0" prefHeight="40.0" prefWidth="138.0">
               <buttons>
                  <TextField fx:id="resultTextField" prefHeight="26.0" prefWidth="123.0" />
               </buttons>
                </ButtonBar>
                <Button fx:id="devideButton" layoutX="170.0" layoutY="66.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="/" />
                <Button fx:id="multiplieButton" layoutX="170.0" layoutY="107.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="×" />
                <Button fx:id="minusButton" layoutX="170.0" layoutY="148.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="-" />
                <Button fx:id="dotButton" layoutX="76.0" layoutY="189.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="." />
                <Button fx:id="resultButton" layoutX="123.0" layoutY="189.0" mnemonicParsing="false" onAction="#resultButtonController" prefHeight="26.0" prefWidth="31.0" text="=" />
                <Button fx:id="plusButton" layoutX="170.0" layoutY="189.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="+" />
                <Button fx:id="eightButton" layoutX="76.0" layoutY="66.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="8">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="nineButton" layoutX="123.0" layoutY="66.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="9">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="fourButton" layoutX="29.0" layoutY="107.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="4">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="fiveButton" layoutX="76.0" layoutY="107.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="5">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="sixButton" layoutX="123.0" layoutY="107.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="6">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="oneButton" layoutX="29.0" layoutY="148.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="1">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="twoButton" layoutX="76.0" layoutY="148.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="2">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="threeButton" layoutX="123.0" layoutY="148.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="3">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="zeroButton" layoutX="29.0" layoutY="189.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="0">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
            <Button fx:id="sinButton" layoutX="29.0" layoutY="225.0" mnemonicParsing="false" onAction="#triangular" prefHeight="26.0" prefWidth="31.0" text="sin" />
            <Button fx:id="cosButton" layoutX="76.0" layoutY="225.0" mnemonicParsing="false" onAction="#triangular" prefHeight="26.0" prefWidth="31.0" text="cos">
               <font>
                  <Font size="11.0" />
               </font>
            </Button>
            <Button fx:id="tanButton" layoutX="123.0" layoutY="225.0" mnemonicParsing="false" onAction="#triangular" prefHeight="26.0" prefWidth="31.0" text="tan">
               <font>
                  <Font size="11.0" />
               </font>
            </Button>
            <Button fx:id="powButton" layoutX="170.0" layoutY="225.0" mnemonicParsing="false" onAction="#activeButtonController" prefHeight="26.0" prefWidth="31.0" text="^" />
            </children>
        </AnchorPane>
    </children>
</VBox>
